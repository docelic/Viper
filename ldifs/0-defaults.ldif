objectClass: top
objectClass: partmanRecipe
data: regular_boot ::
  40 50 100 ext3
  	$primary{ } $bootable{ }
  	method{ format } format{ }
  	use_filesystem{ } filesystem{ ext3 }
  	mountpoint{ /boot }
  .
  500 10000 1000000000 ext3
  	method{ format } format{ }
  	use_filesystem{ } filesystem{ ext3 }
  	mountpoint{ / }
  .
  64 512 300% linux-swap
  	method{ swap } format{ }
  .

# Use 2 spaces on beginning of each continuation line to keep format correct
