objectClass: top
objectClass: partmanRecipe
data: regular_atomic ::
  500 10000 1000000 ext3
  	$primary{ }
  	$bootable{ }
  	method{ format }
  	format{ }
  	use_filesystem{ }
  	filesystem{ ext3 }
  	mountpoint{ / }
  .
  64 512 300% linux-swap
  	method{ swap }
  	format{ }
  .

